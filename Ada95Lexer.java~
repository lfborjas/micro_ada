          { lexical_warnings.add("Advertencia léxica: se encontró '[' en línea "+String.valueOf(yyline+1)+", columna "+String.valueOf(yycolumn+1)+" y se ha reemplazado por '('");
	currentText=yytext();return symbol(sym.LEFTPAR);
          }
        case 195: break;
